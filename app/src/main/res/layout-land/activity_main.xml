<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingHorizontal="32dp"
    tools:context="app.proyecto.tiendeo.MainActivity">


    <TextView
        android:id="@+id/textView8"
        android:layout_width="520dp"
        android:layout_height="22dp"
        android:text="@string/RegistroNombreTienda"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/txtNombreTienda"
        app:layout_constraintEnd_toEndOf="@+id/txtNombreTienda" />

    <EditText
        android:id="@+id/txtNombreTienda"
        android:layout_width="520dp"
        android:layout_height="43dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="12dp"
        android:background="#F2F2F2"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingHorizontal="10dp"
        android:textColorHint="#4B4B4B"
        app:layout_constraintBottom_toTopOf="@+id/guideline21"
        app:layout_constraintEnd_toEndOf="@+id/btregistro"
        app:layout_constraintEnd_toStartOf="@+id/guideline18"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="255dp"
        android:layout_height="22dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="4dp"
        android:text="@string/RegistroCorreo"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/etcorreo"
        app:layout_constraintEnd_toStartOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="255dp"
        android:layout_height="22dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="4dp"
        android:text="@string/RegistroNombre"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/etnombre"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/guideline22"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/etpais"
        android:layout_width="255dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:background="#F2F2F2"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingHorizontal="10dp"
        android:textColorHint="#4B4B4B"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/etnombre"
        android:layout_width="255dp"
        android:layout_height="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:background="#F2F2F2"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingHorizontal="10dp"
        android:textColorHint="#4B4B4B"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/etciudad"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/etcorreo"
        android:layout_width="255dp"
        android:layout_height="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:background="#F2F2F2"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:paddingHorizontal="10dp"
        android:textColorHint="#4B4B4B"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toStartOf="@+id/etpais"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="255dp"
        android:layout_height="22dp"
        android:layout_marginBottom="4dp"
        android:text="@string/RegistroPais"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/etpais"
        app:layout_constraintEnd_toEndOf="@+id/etpais" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="255dp"
        android:layout_height="22dp"
        android:layout_marginBottom="4dp"
        android:text="@string/RegistroRol"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/spinner"
        app:layout_constraintEnd_toEndOf="@+id/spinner" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="255dp"
        android:layout_height="39dp"
        android:layout_marginBottom="8dp"
        android:autofillHints="name"
        android:background="#F2F2F2"
        android:paddingHorizontal="10dp"
        android:spinnerMode="dialog"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="255dp"
        android:layout_height="22dp"
        android:layout_marginBottom="4dp"
        android:text="@string/RegistroCuidad"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/etciudad"
        app:layout_constraintEnd_toEndOf="@+id/etciudad"
        app:layout_constraintTop_toTopOf="@+id/guideline22"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="255dp"
        android:layout_height="22dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="4dp"
        android:text="@string/RegistroContraseÃ±a"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/etcontra"
        app:layout_constraintEnd_toStartOf="@+id/textView9" />

    <EditText
        android:id="@+id/etciudad"
        android:layout_width="255dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:background="#F2F2F2"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingHorizontal="10dp"
        android:textColorHint="#4B4B4B"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/etcontra"
        android:layout_width="255dp"
        android:layout_height="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:background="#F2F2F2"
        android:ems="10"
        android:inputType="textPassword"
        android:paddingHorizontal="10dp"
        android:textColorHint="#4B4B4B"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/spinner"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="120dp"
        android:layout_height="39dp"
        android:text="Registro"
        android:textColor="?android:textColorPrimary"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline17"
        app:layout_constraintStart_toStartOf="@+id/guideline19"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btregistro"
        android:layout_width="255dp"
        android:layout_height="45dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/color_principal"
        android:onClick="onClick"
        android:text="Registrarme"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline20"
        app:layout_constraintEnd_toStartOf="@+id/guideline18"
        app:layout_constraintTop_toTopOf="@+id/guideline21"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btlogin"
        android:layout_width="255dp"
        android:layout_height="45dp"
        android:layout_marginEnd="10dp"
        android:layout_weight="1"
        android:backgroundTint="#CDCFEB"
        android:onClick="Login"
        android:text="Ya tengo una cuenta"
        android:textColor="#2B34B1"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline20"
        app:layout_constraintEnd_toStartOf="@+id/btregistro"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline17"
        app:layout_constraintTop_toTopOf="@+id/btregistro"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.98" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.02" />

</androidx.constraintlayout.widget.ConstraintLayout>